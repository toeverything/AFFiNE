    "nodemon": "^3.0.1",
    "yjs": "^13.6.7"
  },
  "nodemonConfig": {
    "exec": "node",
    "script": "./src/index.ts",
    "nodeArgs": [
      "--loader",
      "ts-node/esm.mjs",
