{
  "name": "@affine/doc-migration",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./src/index.ts",
    "generate": "prisma generate --schema schema.new.prisma && prisma generate --schema schema.old.prisma && prisma generate --schema schema.old.binary.prisma",
    "generate-new": "prisma generate --schema schema.new.prisma",
    "generate-old": "prisma generate --schema schema.old.prisma",
    "generate-old-binary": "prisma generate --schema schema.old.binary.prisma"
  },
  "dependencies": {
    "@affine/env": "workspace:*",
    "@blocksuite/blocks": "0.0.0-20230814155455-ceb5d5d8-nightly",
    "@blocksuite/store": "0.0.0-20230814155455-ceb5d5d8-nightly",
    "@blocksuite/store-0.6.0": "npm:@blocksuite/store@0.6.0",
    "@prisma/client": "5.1.1",
    "fs-extra": "^11.1.1",
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
    "yjs": "^13.6.7"
  },
  "nodemonConfig": {
    "exec": "node",
    "script": "./src/index.ts",
    "nodeArgs": [
      "--loader",
      "ts-node/esm.mjs",
      "--es-module-specifier-resolution",
      "node"
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1"
  }
}
