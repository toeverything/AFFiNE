{
  "name": "infra",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/infra/src",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "dependsOn": ["^build"],
      "inputs": ["{projectRoot}/**/*"],
      "options": {
        "script": "build"
      },
      "outputs": ["{projectRoot}/dist"]
    }
  }
}
