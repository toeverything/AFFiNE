    "ts-node": "^10.9.2",
    "undici": "^6.6.2",
    "uuid": "^9.0.1",
    "vitest": "1.3.1",
    "which": "^4.0.0",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "async-call-rpc": "^6.4.0",
    "electron-updater": "^6.1.9",
    "link-preview-js": "^3.0.5",
    "nanoid": "^5.0.6",
    "yjs": "^13.6.12"
