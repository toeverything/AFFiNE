    "async-call-rpc": "^6.4.2",
    "electron-updater": "^6.2.1",
    "link-preview-js": "^3.0.5",
    "yjs": "^13.6.14"
  },
  "build": {
    "protocols": [
      {
        "name": "affine",
        "schemes": [
          "affine"
        ]
      }
    ]
  },
  "peerDependencies": {
    "ts-node": "*"
  }
}
