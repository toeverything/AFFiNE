    "vite-plugin-static-copy": "^0.17.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "0.34.6",
    "vitest-fetch-mock": "^0.2.2",
    "vitest-mock-extended": "^1.3.1"
  },
